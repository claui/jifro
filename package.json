{
  "name": "jify",
  "version": "0.0.1",
  "description": "An indexed JSON file database",
  "main": "lib/main.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "tsc",
    "lint": "tslint -p .",
