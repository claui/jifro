  "bugs": {
    "url": "https://github.com/mohd-akram/jify/issues"
  },
  "homepage": "https://github.com/mohd-akram/jify#readme",
  "devDependencies": {
    "@types/node": "^10.9.4",
    "rollup": "^0.65.0",
    "rollup-plugin-typescript3": "^1.1.0",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "^3.0.3"
  },
  "dependencies": {
    "os-lock": "^1.0.0",
    "z85": "^0.0.2"
  }
