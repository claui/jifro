{
  "name": "jify",
  "version": "0.1.3",
  "description": "An indexed JSON file database",
  "main": "lib/main.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "tsc",
    "lint": "tslint -p .",
    "profile": "cd test && rollup -c | node --prof --no-logfile-per-isolate && node --prof-process v8.log > data/processed.txt && rm v8.log",
    "test": "TS_NODE_FILES=true ts-node test",
    "prepare": "npm run build",
    "version": "npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mohd-akram/jify.git"
  },
  "keywords": [
    "json",
    "database"
  ],
  "author": "Mohamed Akram",
  "license": "AGPL-3.0-only",
  "bugs": {
    "url": "https://github.com/mohd-akram/jify/issues"
  },
  "homepage": "https://github.com/mohd-akram/jify#readme",
  "devDependencies": {
    "@types/node": "^10.11.3",
    "rollup": "^0.66.2",
    "rollup-plugin-typescript3": "^1.1.1",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "^3.1.1"
  },
  "dependencies": {
    "os-lock": "^1.0.0",
    "z85": "^0.0.2"
  }
}
